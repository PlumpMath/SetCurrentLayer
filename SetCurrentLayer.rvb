Call Main()
Sub Main()
	Dim strObject,strLayer
	strObject = Rhino.GetObject("Pick object to jump to its layer",,True)
	
	If Not IsNull(strObject) Then
		strLayer = Rhino.ObjectLayer(strObject)
		Rhino.CurrentLayer strLayer
	End If
End Sub